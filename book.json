{
  "title": "我的技术栈",
  "description": "笔记",
  "language": "en",
  "plugins": [
    "anchor-navigation-ex",
    "atoc",
    "highlight",
    "-prism",
    "-prism-themes",
    "fontsettings",
    "livereload",
    "sharing",
    "-lunr",
    "-search",
    "search-plus",
    "splitter",
    "tbfed-pagefooter",
    "expandable-chapters-small",
    "advanced-emoji",
    "emphasize",
    "-summary"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": false
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy Guanghui Wang",
      "modify_label": "File Modified：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  },
  "links": {
    "gitbook": false,
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "all": false
    }
  }
}
